<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zj.orderidleone.dao.IOrderidleDao">


    <select id="getOrderDetailList" parameterType="com.zj.orderidleone.model.OrderidleBo" resultType="com.zj.orderidleone.model.OrderidleBo" >
        SELECT
            *
        FROM
             t_order_idtail
        WHERE createPeople=#{createPeople}
    </select>
    <delete id="deleteorderidle" >
        DELETE FROM
            t_order_idtail
        WHERE
            id=#{id}
    </delete>
    <insert id="addOrderidle" parameterType="java.util.HashMap">
        INSERT INTO
            t_order (id,createPeople,sendAddr,orderState,isEvaluate,createDate,updatePeople,
                    updateDate,evaluateDate,evaluateContent,isDelete)
        VALUES
            (#{id},#{createPeople},#{sendAddr},#{orderState},#{isEvaluate},#{createDate},#{updatePeople},
            #{updateDate},#{evaluateDate},#{evaluateContent},#{isDelete})
    </insert>
    <insert id="addOrderDetailidle" parameterType="java.util.List">
            insert into t_order_idtail(
                id,
                orderId,
                price,
                cartNumber,
                fmImg,
                commodityDesc,
                idlezonId,
                createPeople,
                createDate,
                isDelete
            ) values
        <foreach collection="list" item="item" index="index" separator=",">
            (
                #{item.id},
                #{item.orderId},
                #{item.price},
                #{item.cartNumber},
                #{item.fmImg},
                #{item.commodityDesc},
                #{item.idlezonId},
                #{item.createPeople},
                #{item.createDate},
                #{item.isDelete}
            )
        </foreach>
    </insert>




</mapper>
